{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "lint-fix": {
      "outputs": []
    },
    "check-types": {
      "outputs": []
    },
    "dev": {
      "dependsOn": ["generate-css-types"],
      "cache": false
    },
    "generate-css-types": {
      "outputs": ["tailwind-types.ts"]
    }
  }
}
