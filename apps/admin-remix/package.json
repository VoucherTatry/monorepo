{
  "name": "admin-remix",
  "version": "0.0.1",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "build:css": "pnpm run generate:css -- --minify",
    "build:remix": "remix build",
    "dev": "run-p dev:*",
    "dev:css": "pnpm run generate:css -- --watch",
    "dev:remix": "cross-env NODE_ENV=development binode --require ./mocks -- @remix-run/dev:remix dev",
    "shadow-db:start": "docker-compose up -d",
    "shadow-db:stop": "docker-compose stop",
    "format": "prettier --write .",
    "generate:css": "tailwindcss -o ./app/styles/tailwind.css",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "setup": "run-s shadow-db:start setup:wait-shadow-db setup:db setup:seed shadow-db:stop",
    "setup:db": "prisma migrate dev",
    "setup:seed": "prisma db seed",
    "setup:wait-shadow-db": "dotenv -- node ./wait-shadow-db-setup.js",
    "start": "remix-serve build",
    "start:mocks": "dotenv -- binode --require ./mocks -- @remix-run/serve:remix-serve build",
    "check-types": "tsc --noEmit --pretty",
    "postinstall": "pnpm prisma generate --schema ./prisma/schema.prisma"
  },
  "prettier": {
    "singleAttributePerLine": true
  },
  "dependencies": {
    "@heroicons/react": "^2.0.13",
    "@prisma/client": "^4.7.1",
    "@remix-run/node": "^1.8.0",
    "@remix-run/react": "^1.8.0",
    "@remix-run/serve": "^1.8.0",
    "@remix-run/vercel": "^1.8.0",
    "@supabase/auth-helpers-remix": "^0.1.3",
    "@supabase/supabase-js": "^2.1.1",
    "clsx": "^1.2.1",
    "isbot": "^3.6.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix": "^1.8.0",
    "remix-params-helper": "^0.4.10",
    "superjson": "^1.11.0",
    "tiny-invariant": "^1.3.1",
    "ui": "workspace:*",
    "zod": "^3.19.1",
    "zustand": "^4.1.4"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.8.0",
    "@types/eslint": "^8.4.10",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "binode": "^1.0.5",
    "dotenv": "^16.0.3",
    "eslint": "^8.29.0",
    "eslint-config-custom": "workspace:*",
    "msw": "0.49.1",
    "npm-run-all": "^4.1.5",
    "prettier": "2.8.0",
    "prettier-plugin-tailwindcss": "^0.2.0",
    "prisma": "^4.7.1",
    "shared-config": "workspace:*",
    "tailwindcss": "^3.2.4",
    "tailwindcss-classnames": "^3.0.7",
    "ts-node": "^10.9.1",
    "tsconfig": "workspace:*",
    "tsconfig-paths": "^4.1.1",
    "typescript": "^4.9.3"
  },
  "engines": {
    "node": "v16"
  },
  "prisma": {
    "schema": "prisma/schema.prisma",
    "seed": "ts-node --require tsconfig-paths/register prisma/seed.server.ts"
  }
}
