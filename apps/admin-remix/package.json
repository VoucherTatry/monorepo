{
  "name": "admin-remix",
  "version": "0.0.1",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "build": "run-s \"build:*\"",
    "build:css": "pnpm run generate:css --minify",
    "build:remix": "remix build",
    "dev": "run-p \"dev:*\"",
    "dev:css": "pnpm run generate:css --watch",
    "dev:remix": "cross-env NODE_ENV=development remix dev",
    "generate:css": "npx tailwindcss -i ./app/styles/base.css -o ./app/tailwind.css",
    "format": "prettier --write .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "remix-serve build",
    "check-types": "tsc --noEmit --pretty",
    "generate-prisma": "pnpm prisma generate --schema ./prisma/schema.prisma",
    "postinstall": "pnpm run generate-prisma"
  },
  "prettier": {
    "singleAttributePerLine": true
  },
  "dependencies": {
    "@heroicons/react": "^2.0.14",
    "@prisma/client": "^4.9.0",
    "@remix-run/node": "^1.12.0",
    "@remix-run/react": "^1.12.0",
    "@remix-run/serve": "^1.12.0",
    "@remix-run/vercel": "^1.12.0",
    "@supabase/auth-helpers-remix": "^0.1.4",
    "@supabase/supabase-js": "^2.7.0",
    "clsx": "^1.2.1",
    "isbot": "^3.6.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-zorm": "^0.6.1",
    "remix": "^1.12.0",
    "superjson": "^1.12.2",
    "tiny-invariant": "^1.3.1",
    "ui": "workspace:*",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.12.0",
    "@types/eslint": "^8.21.0",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "autoprefixer": "^10.4.13",
    "dotenv": "^16.0.3",
    "eslint": "^8.33.0",
    "eslint-config-custom": "workspace:*",
    "npm-run-all": "^4.1.5",
    "prettier": "2.8.3",
    "prettier-plugin-tailwindcss": "^0.2.2",
    "prisma": "^4.9.0",
    "shared-config": "workspace:*",
    "tailwindcss": "^3.2.4",
    "tailwindcss-classnames": "^3.0.7",
    "ts-node": "^10.9.1",
    "tsconfig": "workspace:*",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": "v16"
  },
  "prisma": {
    "schema": "prisma/schema.prisma",
    "seed": "ts-node --require tsconfig-paths/register prisma/seed.server.ts"
  }
}
