{
  "name": "admin-remix",
  "version": "0.0.1",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:css build:dev",
    "build:css": "yarn run generate:css -- --minify",
    "build:remix": "remix build",
    "dev": "run-p dev:css dev:remix",
    "dev:css": "yarn run generate:css -- --watch",
    "dev:remix": "cross-env NODE_ENV=development binode --require ./mocks -- @remix-run/dev:remix dev",
    "shadow-db:start": "docker-compose up -d",
    "shadow-db:stop": "docker-compose stop",
    "format": "prettier --write .",
    "generate:css": "tailwindcss -o ./app/styles/tailwind.css",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "setup": "run-s shadow-db:start setup:wait-shadow-db setup:db setup:seed shadow-db:stop",
    "setup:db": "prisma migrate dev",
    "setup:seed": "prisma db seed",
    "setup:wait-shadow-db": "dotenv -- node ./wait-shadow-db-setup.js",
    "start": "remix-serve build",
    "start:mocks": "dotenv -- binode --require ./mocks -- @remix-run/serve:remix-serve build",
    "check-types": "tsc --noEmit --pretty"
  },
  "prettier": {
    "singleAttributePerLine": true
  },
  "dependencies": {
    "@heroicons/react": "^2.0.11",
    "@prisma/client": "^4.3.1",
    "@remix-run/node": "^1.7.1",
    "@remix-run/react": "^1.7.1",
    "@remix-run/serve": "^1.7.1",
    "@supabase/supabase-js": "^1.35.6",
    "clsx": "^1.2.1",
    "isbot": "^3.5.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-params-helper": "^0.4.10",
    "superjson": "^1.10.0",
    "tiny-invariant": "^1.2.0",
    "ui": "workspace:*",
    "zod": "^3.19.1",
    "zustand": "^4.1.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.5.0",
    "@remix-run/dev": "^1.7.1",
    "@types/eslint": "^8.4.6",
    "@types/react": "^18.0.20",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.1.0",
    "binode": "^1.0.5",
    "c8": "^7.12.0",
    "dotenv": "^16.0.2",
    "esbuild-register": "^3.3.3",
    "eslint": "^8.23.1",
    "eslint-config-custom": "workspace:*",
    "happy-dom": "^6.0.4",
    "msw": "0.47.3",
    "npm-run-all": "^4.1.5",
    "prettier": "2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "prisma": "^4.3.1",
    "shared-config": "workspace:*",
    "tailwindcss": "^3.1.8",
    "tailwindcss-classnames": "^3.0.6",
    "ts-node": "^10.9.1",
    "tsconfig": "workspace:*",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.3"
  },
  "engines": {
    "node": ">=14"
  },
  "prisma": {
    "schema": "app/core/database/schema.prisma",
    "seed": "ts-node --require tsconfig-paths/register app/core/database/seed.server.ts"
  }
}
